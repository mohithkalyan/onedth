<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/selfcare_bg">

    <Button
        android:id="@+id/button4"
        android:layout_width="184dp"
        android:layout_height="48dp"
        android:layout_marginBottom="320dp"
        android:background="@drawable/balance_dist_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="184dp"
        android:layout_height="48dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/lastrecharges_dist_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

    <Button
        android:id="@+id/button8"
        android:layout_width="184dp"
        android:layout_height="48dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/reqbal_dist_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button5" />

    <Button
        android:layout_width="184dp"
        android:layout_height="48dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/help_dist_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8" />


</androidx.constraintlayout.widget.ConstraintLayout>